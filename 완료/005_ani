```typescript
export const tokenWidgetAnimations = {
  // ì»¨í…Œì´ë„ˆ ì§„ì…
  container: {
    initial: { opacity: 0, y: 20 },
    animate: { opacity: 1, y: 0 },
    transition: { 
      duration: 0.5,
      ease: easingFunctions.easeOutQuart
    }
  },

  // ê¸ˆì•¡ ë³€í™” ì• ë‹ˆë©”ì´ì…˜
  amountChange: {
    key: (amount: number) => amount, // key propìœ¼ë¡œ ë¦¬ë Œë”ë§ íŠ¸ë¦¬ê±°
    initial: { scale: 0.8, opacity: 0 },
    animate: { scale: 1, opacity: 1 },
    transition: { 
      duration: 0.6, 
      type: "spring", 
      stiffness: 100,
      damping: 15
    }
  },

  // ìƒíƒœ ì•„ì´ì½˜ íšŒì „
  statusIcon: {
    animate: (change: string) => ({
      rotate: change === 'increase' ? 360 : 0
    }),
    transition: { 
      duration: 0.8, 
      ease: easingFunctions.easeInOutBack
    }
  },

  // íŠ¸ë Œë“œ ì•„ì´ì½˜ ë“±ì¥
  trendIcon: {
    initial: { scale: 0 },
    animate: { scale: 1 },
    transition: { 
      delay: 0.3, 
      type: "spring", 
      stiffness: 200,
      damping: 12
    }
  },

  // ìƒíƒœë°” ì±„ìš°ê¸°
  statusBar: {
    initial: { width: 0 },
    animate: (status: string) => ({
      width: status === 'critical' ? '20%' : 
             status === 'warning' ? '60%' : '100%'
    }),
    transition: { 
      duration: 1, 
      delay: 0.5,
      ease: easingFunctions.easeOutQuart
    }
  },

  // ìƒíƒœ ë©”ì‹œì§€ ë“±ì¥
  statusMessage: {
    initial: { opacity: 0 },
    animate: { opacity: 1 },
    transition: { 
      delay: 1,
      duration: 0.3
    }
  }
};
```

