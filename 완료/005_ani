export const chatBubbleAnimations = {
  // ì±„íŒ…ì°½ ì—´ê¸°/ë‹«ê¸°
  chatWindow: {
    initial: { opacity: 0, scale: 0.8, y: 20 },
    animate: { opacity: 1, scale: 1, y: 0 },
    exit: { opacity: 0, scale: 0.8, y: 20 },
    transition: { 
      duration: 0.3, 
      type: "spring", 
      stiffness: 200,
      damping: 20
    }
  },

  // ì±„íŒ… ë²„íŠ¼
  chatButton: {
    whileHover: { scale: 1.1 },
    whileTap: { scale: 0.9 },
    transition: springConfigs.snappy
  },

  // ë²„íŠ¼ ì•„ì´ì½˜ ë³€í™”
