export const animationConfig = {
  // ì§„ì… ì• ë‹ˆë©”ì´ì…˜ - í˜ì´ë“œì¸ + ìŠ¬ë¼ì´ë“œ
  entrance: {
    initial: { opacity: 0, y: 20 },
    animate: { opacity: 1, y: 0 },
    transition: { 
      duration: 0.5, 
      ease: [0.25, 0.46, 0.45, 0.94] // easeOutQuart
    }
  },

  // ë¹ ë¥¸ ì§„ì… ì• ë‹ˆë©”ì´ì…˜
  entranceFast: {
    initial: { opacity: 0, y: 10 },
    animate: { opacity: 1, y: 0 },
    transition: { 
      duration: 0.3, 
      ease: [0.25, 0.46, 0.45, 0.94]
    }
  },

  // ëŠë¦° ì§„ì… ì• ë‹ˆë©”ì´ì…˜
  entranceSlow: {
    initial: { opacity: 0, y: 30 },
    animate: { opacity: 1, y: 0 },
    transition: { 
      duration: 0.8, 
      ease: [0.25, 0.46, 0.45, 0.94]
    }
  },

  // ì¢Œì¸¡ì—ì„œ ìŠ¬ë¼ì´ë“œ
  slideInLeft: {
    initial: { opacity: 0, x: -50 },
    animate: { opacity: 1, x: 0 },
    transition: { 
      duration: 0.6, 
      ease: [0.25, 0.46, 0.45, 0.94]
    }
  },

  // ìš°ì¸¡ì—ì„œ ìŠ¬ë¼ì´ë“œ
  slideInRight: {
    initial: { opacity: 0, x: 50 },
    animate: { opacity: 1, x: 0 },
    transition: { 
      duration: 0.6, 
      ease: [0.25, 0.46, 0.45, 0.94]
    }
  },

  // í‡´ì¥ ì• ë‹ˆë©”ì´ì…˜
  exit: {
    initial: { opacity: 1, scale: 1 },
    exit: { opacity: 0, scale: 0.95 },
    transition: { 
      duration: 0.2, 
      ease: [0.4, 0, 1, 1]
    }
  }
};
```

